<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seminlee.tic_tac_toe2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe2">
        <activity android:name="com.seminlee.tic_tac_toe2.WinPage2" />
        <activity android:name="com.seminlee.tic_tac_toe2.WinPage" />
        <activity android:name="com.seminlee.tic_tac_toe2.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.seminlee.tic_tac_toe2.TwoPlayerActivity1" />
        <activity android:name="com.seminlee.tic_tac_toe2.SinglePlayerActivity1" />
        <activity android:name="com.seminlee.tic_tac_toe2.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>